<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
        package="com.luna.application">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE"></uses-permission>

    <application
            android:allowBackup="true"
            android:icon="@drawable/ic_inviter"
            android:label="Luna"
            android:roundIcon="@mipmap/ic_inviter"
            android:supportsRtl="true"
            android:theme="@style/AppTheme">
        <activity android:name="com.luna.application.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="com.luna.application.didi.DiDiActivity" />
        <activity android:name="com.luna.application.calculate.CalculateActivity" />
        <activity android:name="com.luna.application.login.LoginActivity" />
        <activity android:name="com.luna.application.login.InfoActivity" />
        <activity android:name="com.luna.application.hero.HeroActivity" />
        <activity android:name="com.luna.application.hero.MainWeaponActivity" />
        <activity android:name="com.luna.application.hero.DeputyWeapon" />
        <activity android:name="com.luna.application.LogActivity" />
        <activity android:name="com.luna.application.dailog.DialogActivity" />
        <activity android:name="com.luna.application.file.FileActivity" />
        <activity android:name="com.luna.application.listView.ListViewActivity" />
        <activity android:name="com.luna.application.listView.exam.ExamActivity" />
    </application>

</manifest>